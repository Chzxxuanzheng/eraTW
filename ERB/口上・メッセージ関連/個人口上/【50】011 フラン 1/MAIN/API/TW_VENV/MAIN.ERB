;base变动信息提示
@k1011BaseChangeInfoTw(baseId, chara, num)
	#DIM baseId
	#DIM chara
	#DIM num
	#DIM DYNAMIC hasPrintFlag
	;没有变动直接返回
	SIF !num
		RETURN
	;体力
	IF baseId == 0
		IF num > 0
			SETCOLOR 0xFFFF00
			SIF num > 150
				SETCOLOR 0xFF0000
			PRINTFORM %BASENAME:baseId%-{num}
		ELSE
			SETCOLOR 0x00FF00
			PRINTFORM %BASENAME:baseId%+{-num}
		ENDIF
		hasPrintFlag = 1
	ENDIF
	IF DOWNBASE:LOCAL:気力
		IF DOWNBASE:LOCAL:気力 > 0
			SETCOLOR 0xFFFF00
			SIF DOWNBASE:LOCAL:気力 > 150
				SETCOLOR 0xFF0000
			PRINTFORM 気力-{DOWNBASE:LOCAL:気力} 
		ELSE
			SETCOLOR 0x00FF00
			PRINTFORM 気力+{-DOWNBASE:LOCAL:気力}
		ENDIF
		hasPrintFlag = 1
	ENDIF
	RESETCOLOR
	SIF hasPrintFlag
		PRINTFORML (%CALLNAME:chara%)

;exp变动
@k1011ExpChangeInfoTw(nameOfExp, addNum, chara)
	#DIM chara
	#DIM addNum
	#DIMS nameOfExp
	#DIM expId
	#DIM atExp
	#DIM nextExp
	;啥经验都没有加
	SIF !addNum
		RETURN
	IF nameOfExp == "吸血经验"
		SIF k1011GetExp("吸血经验") > EXPLV:15
			RETURN
		atExp = GETEXPLV(k1011GetExp("吸血经验"), 15)
		nextExp = atExp + 1
		PRINTFORM 吸血经验 +{addNum}(%CALLNAME:chara%)
		CALL PRINTLEVELBAR(k1011GetExp("吸血经验"), EXPLV:nextExp, atExp, nextExp)
		PRINTL 
	ELSE
		;获取exp的id
		expId = GETNUM(EXP, nameOfExp)
		SIF LOCAL:2 == 1
			PRINTL
		PRINTFORM %EXP_TR(expId)% +{addNum}(%CALLNAME:chara%)
		CALL Qol_SkillProgressBar(chara, expId)
		PRINTL
	ENDIF

